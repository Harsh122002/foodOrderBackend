{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/groups/(.*)",
      "dest": "/groups/$1"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "app.js"
    }
  ],
  "functions": {
    "app.js": {
      "includeFiles": "groups/**"
    }
  }
}
